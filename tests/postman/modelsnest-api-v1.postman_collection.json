              "raw": "{\n  \"model\": \"deepseek\",\n  \"messages\": [\n    { \"role\": \"user\", \"content\": \"Explain quantum computing simply in 3 bullets.\" }\n  ],\n  \"max_tokens\": 250,\n  \"temperature\": 0.4\n}"
